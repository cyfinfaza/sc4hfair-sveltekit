{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-jsdoc"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"experimentalTernaries": true,
	"quoteProps": "consistent",
	"jsdocCapitalizeDescription": false
}
